      "id": "inputTPED",
      "details": {
        "label": "Input .tped (relative to /iplant/home)",
        "showAttribute": false
      },
      "semantics": {
        "minCardinality": 0,
        "ontology": [
          "http://sswapmeet.sswap.info/soybase/"
        ]
      },
      "value": {
        "visible": true,
        "required": false
      }
    },
    {
      "id": "inputFAM",
      "details": {
