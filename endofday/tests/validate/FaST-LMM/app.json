  "parameters": [
	{
      "id": "verboseOutput",
      "details": {
        "label": "Triggers verbose mode",
        "showAttribute": false
      },
      "semantics": {
        "ontology": [
          "xs:bool"
        ]
      },
      "value": {
        "default": 0,
        "type": "number",
        "visible": true,
        "required": false
      }
    },
    {
      "id": "C",
      "details": {
        "label": "The covariate file is an input",
        "showAttribute": false
      },
      "semantics": {
        "ontology": [
          "xs:bool"
        ]
      },
      "value": {
        "default": 0,
        "type": "number",
        "visible": true,
        "required": false
      }
    },
    {
      "id": "MainFileset",
      "details": {
        "label": "The fileset used as the main inputs",
		"description": "The fileset used as the main inputs: P for PEDMAP, B for BEDBIMFAM, or T for TPEDTFAM",
        "showAttribute": false
      },
      "semantics": {
        "ontology": [
          "xs:string"
        ]
      },
      "value": {
        "default": "B",
        "type": "number",
        "visible": true,
        "required": false
      }
    },
	{
      "id": "SimFileset",
      "details": {
        "label": "Fileset for similarity matrix",
	"description": "The PLINK file group used to calculate the genetic similarity matrix (PEDMAP, BEDBIMFAM, or TPEDTFAM)",
        "showAttribute": false
      },
      "semantics": {
        "ontology": [
          "xs:string"
        ]
      },
      "value": {
        "default": "PEDMAP",
        "type": "string",
        "visible": true,
        "required": false
      }
    },
    {
      "id": "mpheno",
      "details": {
        "label": "Proper column number for phenotype file",
        "showAttribute": false
      },
      "semantics": {
        "ontology": [
          "xs:integer"
        ]
      },
      "value": {
        "default": 1,
        "type": "number",
        "visible": true,
        "required": false
      }
    },
	{
      "id": "output",
      "details": {
        "label": "The name of the analysis output file",
        "showAttribute": false
      },
      "semantics": {
        "ontology": [
          "http://sswapmeet.sswap.info/soybase/"
        ]
      },
      "value": {
        "default": "MyFastLmmResults",
        "type": "string",
        "visible": true,
        "required": true
      }
    }
  ],
  "checkpointable": false
